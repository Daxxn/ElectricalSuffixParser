<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Title>Electrical Suffix Parser</Title>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<GenerateDocumentationFile>True</GenerateDocumentationFile>
		<Version>1.1.0</Version>
		<RepositoryUrl>https://github.com/Daxxn/ElectricalSuffixParser.git</RepositoryUrl>
		<PackageTags>parser; electrical;</PackageTags>
		<Description>Electrical suffix parser for console or WPF applications.</Description>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="SettingsLibrary" Version="2.2.0" />
	</ItemGroup>

</Project>
